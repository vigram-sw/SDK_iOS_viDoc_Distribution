{"schemaVersion":{"patch":0,"minor":3,"major":0},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/vigramsdk\/useresetvidoc"]}],"sections":[],"metadata":{"title":"Use Reset viDoc","roleHeading":"Article","images":[{"identifier":"logo.png","type":"icon"}],"modules":[{"name":"VigramSDK"}],"role":"article"},"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/UseResetVidoc"},"hierarchy":{"paths":[["doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK"]]},"kind":"article","primaryContentSections":[{"kind":"content","content":[{"level":2,"anchor":"","text":"","type":"heading"},{"inlineContent":[{"type":"text","text":"The viDoc reset procedure returns the settings to factory defaults."}],"type":"paragraph"},{"inlineContent":[{"inlineContent":[{"type":"text","text":"Note:"}],"type":"strong"},{"type":"text","text":" Perform this procedure only if absolutely necessary."}],"type":"paragraph"},{"content":[{"inlineContent":[{"type":"text","text":"The reset procedure takes approximately 1 minute."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"At the beginning of the process, all messages are interrupted."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Disconnection from all satellites occurs."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"After the viDoc is reset, sending messages resumes."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"You must wait to receive the full NMEA GGA message."}],"type":"paragraph"}],"name":"Note","style":"note","type":"aside"},{"inlineContent":[{"type":"text","text":"To track the reset process, you need to use publisher"}],"type":"paragraph"},{"code":["private var cancellables = Set<AnyCancellable>()","","peripheral.viDocResetState","    .sink(receiveCompletion: { completion in","        switch completion {","        case .finished:","            break \/\/ normal completion","        case .failure(let error):","            \/\/ do something with error","        }","    }) { [weak self] viDocResetState in","            switch viDocResetState {","            case let .isReseting(value):","                if value {","                    \/\/ reset in progress","                } else {","                    \/\/ reset is finished","                }","            case .failure: ","                \/\/ Error of reset viDoc","                break","            }","    }.store(in: &cancellables)"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"text","text":"To start the reset process, you need to perform the following method:"}],"type":"paragraph"},{"code":["peripheral.resetViDoc()"],"syntax":"swift","type":"codeListing"}]}],"references":{"doc://com.vigram.VigramSDK/documentation/VigramSDK":{"role":"collection","images":[{"identifier":"logo.png","type":"icon"}],"url":"\/documentation\/vigramsdk","type":"topic","title":"VigramSDK","abstract":[],"kind":"symbol","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK"},"logo.png":{"identifier":"logo.png","alt":"Vigram logo.","type":"image","variants":[{"traits":["1x","light"],"url":"\/images\/com.vigram.VigramSDK\/logo.png"}]}}}