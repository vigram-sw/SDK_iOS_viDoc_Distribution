)","","if directory.count != 0  {","    path = directory[0].appendingPathComponent(\"Name directory\")","    if let path = path?.path {","        if !fileManager.fileExists(atPath: path) {","            do {","                try fileManager.createDirectory(","                    atPath: path, ","                    withIntermediateDirectories: true, ","                    attributes: nil","                )","            } catch {","                print(\"Error: \\(error.localizedDescription)\")","            }","        }","    }","    path = path?.appendingPathComponent(file)","}"]},{"inlineContent":[{"text":"After then, start record measurements perform this method","type":"text"}],"type":"paragraph"},{"syntax":"swift","code":["peripheral.startRecordPPKMeasurements(url: ubxFilePath)"],"type":"codeListing"},{"type":"paragraph","inlineContent":[{"text":"Record is active.","type":"text"},{"text":" ","type":"text"},{"text":"If you need to stop record measurements perform this method","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["peripheral.stopRecordPPKMeasurements()"]},{"inlineContent":[{"type":"text","text":"All messages will saving to file"}],"type":"paragraph"}]}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/vigramsdk\/useppkmeasurements"]}],"references":{"logo.png":{"identifier":"logo.png","variants":[{"traits":["1x","light"],"url":"\/images\/com.vigram.VigramSDK\/logo.png"}],"alt":"Vigram logo.","type":"image"},"doc://com.vigram.VigramSDK/documentation/VigramSDK":{"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK","abstract":[],"title":"VigramSDK","type":"topic","role":"collection","images":[{"type":"icon","identifier":"logo.png"}],"url":"\/documentation\/vigramsdk","kind":"symbol"}}}