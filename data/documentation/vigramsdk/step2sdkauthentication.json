 .finished:","                    break \/\/ normal receive value","                case .failure(let error):","                    \/\/ failure to receive value","                    \/\/ do something with error publisher","                    print(error.localizedDescription)","                }","            }) { [weak self] resultAuthentication in","                switch resultAuthentication {","                    case .success:","                        \/\/ do something after successfull authentication","                        \/\/ for example init Vigram services and","                        \/\/ start bluetooth scaning after than","                    case .failure(let error):","                        \/\/ do something with error authentication","                        print(error.localizedDescription)","                }","            }","    }","","    deinit {","        autentication = nil","    }","}"],"type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"text":"Return value:","type":"text"}]},{"type":"text","text":" Result<Bool, AuthenticationError>"}]},{"inlineContent":[{"inlineContent":[{"text":"Throw error:","type":"text"}],"type":"strong"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/AuthenticationError","isActive":true}],"type":"paragraph"},{"anchor":"","text":"","type":"heading","level":3},{"inlineContent":[{"type":"text","text":"It is recommended to place the authentication call in the initialization of your Vigram helper class to avoid missing the token expiration date (20 days offline)."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"This completes the authentication procedure."}],"type":"paragraph"},{"inlineContent":[{"text":"The next step is ","type":"text"},{"type":"strong","inlineContent":[{"overridingTitle":"change the configuration parameters.","isActive":true,"type":"reference","identifier":"doc:\/\/com.v