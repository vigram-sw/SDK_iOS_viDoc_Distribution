{"sections":[],"hierarchy":{"paths":[["doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK"]]},"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/UsePeripheralConfiguration"},"schemaVersion":{"minor":3,"patch":0,"major":0},"metadata":{"roleHeading":"Article","title":"Use Peripheral Configuration","images":[{"identifier":"logo.png","type":"icon"}],"modules":[{"name":"VigramSDK"}],"role":"article"},"variants":[{"paths":["\/documentation\/vigramsdk\/useperipheralconfiguration"],"traits":[{"interfaceLanguage":"swift"}]}],"kind":"article","primaryContentSections":[{"kind":"content","content":[{"text":"","level":2,"type":"heading","anchor":""},{"inlineContent":[{"text":"If you need to disable some of the GNSS messages or change the rate of changing message or minimum elevation value use in navigation parameters, create a variable of type ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/PeripheralConfiguration"},{"text":" that includes the configuration data and receive this parameter to the peripheral initializer.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"Example:","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["let peripheralConfiguration = ","    PeripheralConfiguration(rateOfChangeMessages: .hertz1,","                            glonassActivate: false)"]},{"type":"paragraph","inlineContent":[{"text":"After then, need to pass configuration to peripheral init:","type":"text"}]},{"code":["\/\/ If use logger","peripheral = ","    Vigram.peripheral(peripheral, log: path,","                      configuration: peripheralConfiguration)","","\/\/ If not use logger","peripheral = ","    Vigram.peripheral(peripheral,","                      configuration: peripheralConfiguration)","","\/\/ If not use configuration and logger","peripheral = Vigram.peripheral(peripheral)"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"Note: If this parameter is not recieved to the initializer, all parameters will be set by default."}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["\/\/ Rate of changing message is set 7 hertz","rateOfChangeMessages: RateValue = .hertz7","","\/\/ Minimum elevation value using in navigation is set 10 degree","elevationValue: ElevationValue = .ang10,","","\/\/ All satellite constellation is active","glonassActivate = true","beidouActivate = true","galileoActivate = true","qzssActivate = true","sbassActivate = true","","\/\/ NAV-DOP and NAV-PVT Messages is active","navDOPActivate = true","navPVTActivate = true","","\/\/ Dynamic state mode","dynamicType = .pedestrian"]}]}],"references":{"doc://com.vigram.VigramSDK/documentation/VigramSDK/PeripheralConfiguration":{"fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"PeripheralConfiguration"}],"type":"topic","navigatorTitle":[{"text":"PeripheralConfiguration","kind":"identifier"}],"title":"PeripheralConfiguration","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/PeripheralConfiguration","role":"symbol","abstract":[{"text":"Initial configuration for peripheral","type":"text"}],"kind":"symbol","url":"\/documentation\/vigramsdk\/peripheralconfiguration"},"logo.png":{"alt":"Vigram logo.","variants":[{"url":"\/images\/com.vigram.VigramSDK\/logo.png","traits":["1x","light"]}],"type":"image","identifier":"logo.png"},"doc://com.vigram.VigramSDK/documentation/VigramSDK":{"kind":"symbol","role":"collection","title":"VigramSDK","type":"topic","images":[{"identifier":"logo.png","type":"icon"}],"url":"\/documentation\/vigramsdk","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK","abstract":[]}}}