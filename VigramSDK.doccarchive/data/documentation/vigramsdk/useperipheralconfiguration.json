{"metadata":{"modules":[{"name":"VigramSDK"}],"roleHeading":"Article","title":"Use Peripheral Configuration","images":[{"type":"icon","identifier":"logo.png"}],"role":"article"},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/vigramsdk\/useperipheralconfiguration"]}],"kind":"article","schemaVersion":{"minor":3,"patch":0,"major":0},"primaryContentSections":[{"kind":"content","content":[{"level":2,"type":"heading","anchor":"","text":""},{"type":"paragraph","inlineContent":[{"text":"If you need to disable some of the GNSS messages or change the rate of changing message or minimum elevation value use in navigation parameters, create a variable of type ","type":"text"},{"type":"reference","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/PeripheralConfiguration","isActive":true},{"text":" that includes the configuration data and receive this parameter to the peripheral initializer.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Example:"}]},{"syntax":"swift","code":["let peripheralConfiguration = PeripheralConfiguration(","    rateOfChangeMessages: .hertz1,","    glonassActivate: false",")"],"type":"codeListing"},{"type":"paragraph","inlineContent":[{"text":"After then, need to pass configuration to peripheral init:","type":"text"}]},{"syntax":"swift","code":["\/\/ If use logger","peripheral = Vigram.peripheral(","    peripheral,","    log: path,","    configuration: peripheralConfiguration",")","","\/\/ If not use logger","peripheral = Vigram.peripheral(","    peripheral,","    configuration: peripheralConfiguration",")","","\/\/ If not use configuration and logger","peripheral = Vigram.peripheral(peripheral)"],"type":"codeListing"},{"inlineContent":[{"type":"text","text":"Note: If this parameter is not recieved to the initializer, all parameters will be set by default."}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["\/\/ Rate of changing message is set 7 hertz","rateOfChangeMessages: RateValue = .hertz7","","\/\/ Minimum elevation value using in navigation is set 10 degree","elevationValue: ElevationValue = .ang10,","","\/\/ All satellite constellation is active","glonassActivate = true","beidouActivate = true","galileoActivate = true","qzssActivate = true","sbassActivate = true","","\/\/ NAV-DOP and NAV-PVT Messages is active","navDOPActivate = true","navPVTActivate = true","","\/\/ Dynamic state mode","dynamicType = .pedestrian"]}]}],"hierarchy":{"paths":[["doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK"]]},"identifier":{"url":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/UsePeripheralConfiguration","interfaceLanguage":"swift"},"sections":[],"references":{"logo.png":{"alt":"Vigram logo.","variants":[{"url":"\/images\/com.vigram.VigramSDK\/logo.png","traits":["1x","light"]}],"type":"image","identifier":"logo.png"},"doc://com.vigram.VigramSDK/documentation/VigramSDK/PeripheralConfiguration":{"role":"symbol","abstract":[{"type":"text","text":"Initial configuration for peripheral"}],"kind":"symbol","url":"\/documentation\/vigramsdk\/peripheralconfiguration","title":"PeripheralConfiguration","navigatorTitle":[{"text":"PeripheralConfiguration","kind":"identifier"}],"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/PeripheralConfiguration","type":"topic","fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"text":"PeripheralConfiguration","kind":"identifier"}]},"doc://com.vigram.VigramSDK/documentation/VigramSDK":{"images":[{"type":"icon","identifier":"logo.png"}],"role":"collection","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK","url":"\/documentation\/vigramsdk","abstract":[],"type":"topic","kind":"symbol","title":"VigramSDK"}}}