on in ","        switch completion {","        case .finished:","            \/\/ Value successfully received","            \/\/ You can use this state for any further actions ","            \/\/ after receiving the values.","            break","        case let .failure(error):","            \/\/ Error getting value","            \/\/ Do something with error","            print(error.localizedDescription)","        }","    }) { singlePointValue in","        \/\/ do somethind with singlePointValue","    }.store(in: &subscription)"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"inlineContent":[{"type":"text","text":"Receive value:"}],"type":"strong"},{"text":" ","type":"text"},{"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/SinglePoint","isActive":true,"type":"reference"}],"type":"paragraph"},{"inlineContent":[{"inlineContent":[{"text":"Throw error:","type":"text"}],"type":"strong"},{"type":"text","text":" "},{"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/SinglePointRecordingError","type":"reference","isActive":true}],"type":"paragraph"},{"type":"heading","text":"More information about","level":3,"anchor":"More-information-about"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/SinglePoint","type":"reference","isActive":true}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"reference","isActive":true,"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/EnvironmentData"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/EnviromentDataService","overridingTitleInlineContent":[{"type":"text","text":"EnviromentDataService"}],"type":"reference","isActive":true,"overridingTitle":"EnviromentDataService"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"overridingTitleInlineContent":[{"type":"text","text":"GPSService"}],"type":"reference","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/Step6UsingNTRIP","overridingTitle":"GPSService","isActive":true}]}]},{"content":[{"inlineContent":[{"isActive":true,"overridingTitleInlineContent":[{"type":"text","text":"LaserService"}],"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/Step7LaserService","type":"reference","overridingTitle":"LaserService"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"overridingTitleInlineContent":[{"text":"Use GPSCoordinate + Transformation","type":"text"}],"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/GPSCoordinate","isActive":true,"type":"reference","overridingTitle":"Use GPSCoordinate + Transformation"}]}]},{"content":[{"inlineContent":[{"isActive":true,"overridingTitleInlineContent":[{"type":"text","text":"Using Combine and Publishers"}],"type":"reference","overridingTitle":"Using Combine and Publishers","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/UseCombineAndPublishers"}],"type":"paragraph"}]}]},{"inlineContent":[{"type":"text","text":"If needed to stop measurement process, perform stopMeasurement method:"}],"type":"paragraph"},{"inlineContent":[{"text":"Example:","type":"text"}],"type":"paragraph"},{"code":["singlePointRecordingService.stopMeasurement()"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"text":"If needed to cancel measurement process, perform cancelMeasurement method:","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"Example:","type":"text"}],"type":"paragraph"},{"code":["singlePointRecordingService.cancelMeasurement()"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"If needed to watch mesurement duration time, use variable measurementTime:"}],"type":"paragraph"},{"code":["var measurementTime: StatePublisher<TimeInterval> { get }"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"type":"text","text":"Example:"}],"type":"paragraph"},{"code":["singlePointRecordingService","    .measurementTime","    .sink { value in","        \/\/ do somethind with value","    }.store(in: &subscription)"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"inlineContent":[{"text":"All methods and functions specified in this manual are also available in the ","type":"text"},{"type":"reference","identifier":"https:\/\/github.com\/vigram-sw\/SDK_iOS_viDoc_Distribution\/tree\/demo\/","isActive":true}],"type":"emphasis"}],"type":"paragraph"},{"inlineContent":[{"inlineContent":[{"text":"An important section is ","type":"text"},{"overridingTitle":"Use Antenna Offsets","overridingTitleInlineContent":[{"type":"text","text":"Use Antenna Offsets"}],"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/UseAntennaOffsets","isActive":true,"type":"reference"}],"type":"strong"},{"text":" ","type":"text"},{"text":"Note: Offsets have a data type ","type":"text"},{"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/simd3","type":"reference","isActive":true}],"type":"paragraph"},{"inlineContent":[{"text":"You can set your own custom offsets","type":"text"}],"type":"paragraph"},{"code":["let yourCustomOffset = SIMD3(x: 4.4, y: -1.6, z: 2) \/ 100"],"type":"codeListing","syntax":"swift"},{"type":"heading","text":"Detailed listing Single Point measurements with laser","level":2,"anchor":"Detailed-listing-Single-Point-measurements-with-laser"},{"code":["\/\/ Bottom laser selected (or .back if needed)","let typeOfLaser: LaserConfiguration.Position = .bottom","","\/\/ Laser measurement mode - fast","let laserShotMode: LaserConfiguration.ShotMode = .fast ","","\/\/ Measurement duration (in seconds). ","\/\/ For viDoc protocol version < 2, the measurement duration","\/\/ must be in the range [5...60] seconds.","\/\/","\/\/ For protocol >= 2, any value can be specified. If 0 is ","\/\/ specified, the measurement will continue ","\/\/ indefinitely.","\/\/ ","\/\/ More details in section №8 (Using Single point measurements)","let duration: Double = 30","","\/\/ Creating a laser configuration. ","\/\/ The laser configuration. Keep in mind, that you will not receive ","\/\/ further environment data values after the laser duration has passed.","let laserConfig = LaserConfiguration.init(shotMode: laserShotMode, ","                                          position: typeOfLaser, ","                                          duration: duration)","","\/\/ Antenna offset","\/\/ The offset of the laser in relation to the antenna position. ","\/\/ Use values from AntennaOffset here.","let currentOffsets = AntennaOffset.Laser.iPhone14ProMaxBottom","","\/\/ Measurement method","\/\/ UseDeviceMotion - This attribute specifies whether to incorporate ","\/\/ the device motion into the calculation of the corrected coordinate. ","\/\/ This allows to not only correct coordinates by strictly subtracting ","\/\/ the laser distance measurement from the measured antenna position,","\/\/ but rather enables diagonally held laser measurements as well.","let method = ","    CoordinateCorrection.Method.laser(configuration: laserConfig, ","                                      antennaOffset: offset,","                                      useDeviceMotion: true)","","\/\/ Creates an EnvironmentDataService to keep track of all the environment ","\/\/ information from the device’s internal sensors and possible values  ","\/\/ from the viDoc sensors.","\/\/","\/\/ DynamicStateType - Pedestrian or Stationary type of dynamic state","let environmentDataService = ","    Vigram.environmentDataService(gpsService: gpsService!, ","                                  laserService: laserService!,","                                  peripheral: peripheral!, ","                                  dynamicStateType: .stationary)","","\/\/ Create SinglePoint Recording Service","let singlePointRecordingService = ","    Vigram.singlePointRecordingService(environmentDataService: ","                                        environmentDataService!)","","\/\/ Start SinglePoint measurements","let cancellable = singlePointRecordingService!","    .startMeasurement(duration: duration, ","                      updateInterval: 0.1, with: method)","    .sink { completion in","        switch completion {","        case .finished:","            \/\/ Value successfully received","            \/\/ You can use this state for any further actions ","            \/\/ after receiving the values.","            break","        case let .failure(error):","            \/\/ Error getting value","            \/\/ Do something with error","            print(error.localizedDescription)","        }","    } receiveValue: { singlePoint in","        \/\/ Do something with singlePoint","    }","","\/\/ For stop measurement use this method","\/\/ Important! Use it only when setting the time to 0 (infinite ","\/\/ measurement). After executing this method, the measurement ","\/\/ will end.","singlePointRecordingService!.stopMeasurement()","","\/\/ Important! Use only when time is set to 0 ","\/\/ (infinite measurement). ","\/\/ After this method is executed, the measurement will stop. ","\/\/ The data will NOT BE SAVED.","singlePointRecordingService?.cancelMeasurement()","","\/\/ If you are using an infinite measurement (duration - 0) ","\/\/ and need to track the time of the dimension,","\/\/ use this publisher.","\/\/ Note: The time will starts after the first successful laser ","\/\/ measurement (quality >= acceptable)","let cancellable = singlePointRecordingService!","    .measurementTime","    .sink { time in","        \/\/ do something with measurement time","    }"],"type":"codeListing","syntax":"swift"},{"type":"heading","text":"Detailed listing Single Point measurements without laser","level":2,"anchor":"Detailed-listing-Single-Point-measurements-without-laser"},{"code":["\/\/ The distance from the antenna to the ground to subtract from the ","\/\/ antenna’s coordinate’s altitude.","let distanceToGround = Double(distanceToGround)","","\/\/ Antenna offset","\/\/ The offset of the laser in relation to the antenna position. ","\/\/ Use values from AntennaOffset here.","let currentOffsets = AntennaOffset.SPC.iPhone14ProMaxBottom","","\/\/ Measurement method","\/\/ When using the constant height coordinate correction,","\/\/ the correctedCoordinate property of EnvironmentData will reflect ","\/\/ the position of the antenna with an altitude subtracted by the ","\/\/ constant value.","let method = ","    CoordinateCorrection.Method","        .constant(distanceFromAntennaToGround: antennaDistanceToGround)","","\/\/ Creates an EnvironmentDataService to keep track of all the environment ","\/\/ information from the device’s internal sensors and possible values  ","\/\/ from the viDoc sensors.","\/\/","\/\/ DynamicStateType - Pedestrian or Stationary type of dynamic state","let environmentDataService = ","    Vigram.environmentDataService(gpsService: gpsService!, ","                                  peripheral: peripheral!, ","                                  dynamicStateType: .stationary)","","\/\/ Create SinglePoint Recording Service","let singlePointRecordingService = ","    Vigram.singlePointRecordingService(environmentDataService: ","                                        environmentDataService!)","","let cancellable = singlePointRecordingService!","    .startMeasurement(duration: duration, ","                      updateInterval: 0.1, with: method)","    .sink { completion in","        switch completion {","        case .finished:","            \/\/ Value successfully received","            \/\/ You can use this state for any further actions ","            \/\/ after receiving the values.","            break","        case let .failure(error):","            \/\/ Error getting value","            \/\/ Do something with error","            print(error.localizedDescription)","        }","    } receiveValue: { singlePoint in","        \/\/ Do something with singlePoint","    }"],"type":"codeListing","syntax":"swift"},{"type":"heading","text":"SinglePointRecordingError","level":3,"anchor":"SinglePointRecordingError"},{"inlineContent":[{"isActive":true,"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/SinglePointRecordingError","type":"reference"},{"type":"text","text":" are thrown, when due to the filtering phase of single point recordings there is not enough valid data to create a singlePoint. To investigate possible issues with the measurement, have a look at the provided associated data."}],"type":"paragraph"},{"code":["public enum SinglePointRecordingError: Error, Hashable {","","\/\/ The measurement failed due to too much filtered, non-valid data","\/\/","\/\/ Available in SDK ver. 1.0.3","case nonValidData","","\/\/ This method is not supported in current software version of ","\/\/ the RTK device","\/\/","\/\/ Available in SDK ver. 1.0.3","case requestIsNotSupported(String)","","\/\/ The correction method used has incorrect settings","\/\/ Most likely the CoordinateCorrectionMethod settings ","\/\/ were not selected and passed to the write method correctly","\/\/","\/\/ Available in SDK ver. 1.0.3","case incorrectCoordinateCorrectionMethod(String)","","\/\/ Running measurement process not found","\/\/","\/\/ Available in SDK ver. 1.0.3","case noRunningMeasurementMethod","","\/\/ The value of horizontal accuracy is too high","\/\/ Hor.Accuracy < Configuration.maximumSinglePointHorizontalAccuracy","\/\/","\/\/ Available in SDK ver. 1.0.3","case horizontalAccuracyIsTooHigh","","\/\/ The value of vertical accuracy is too high","\/\/ Vert.Accuracy < Configuration.maximumSinglePointVerticalAccuracy","\/\/","\/\/ Available in SDK ver. 1.0.3","case verticalAccuracyIsTooHigh","","\/\/ The value of altitude difference is too high","\/\/ Altitude < Configuration.maximumSinglePointAltitudeDifference","\/\/","\/\/ Available in SDK ver. 1.0.3","case altitudeDifferenceIsTooHigh","","\/\/ The laser measurements are of bad quality","\/\/","\/\/ This means that most of the laser measurements were of bad ","\/\/ quality or incorrect.","\/\/","\/\/ Available in SDK ver. 1.0.3","case laserMeasurementsIsBadQuality","","\/\/ The measurement cancelled by user.","case measurementCancelled","","\/\/ Deprecated!","\/\/","\/\/ The measurement failed due to too much filtered, non-valid data","\/\/ Most of the measured data have low accuracy.","\/\/ e.g. One or more conditions are not met","\/\/ Hor.Accuracy < Configuration.maximumSinglePointHorizontalAccuracy,","\/\/ Vert.Accuracy < Configuration.maximumSinglePointVerticalAccuracy","\/\/ Altitude < Configuration.maximumSinglePointAltitudeDifference","\/\/ The returned RAW value of this error is the received measurements.","\/\/","\/\/ This error was used in older versions of SDK. Will not be","\/\/ supported in the future","case badMeasurement([EnvironmentData])","","\/\/ Deprecated!","\/\/","\/\/ The measurement failure.","\/\/ In most cases this error is not returned. ","\/\/ If you get it, check if you passed the correct ","\/\/ CoordinateCorrection.Method to the SinglePointRecordingService ","\/\/ measurement procedure (record or startMeasurements).","\/\/","\/\/ This error was used in older versions of SDK. Will not be","\/\/ supported in the future","case measurementFailure","}"],"type":"codeListing","syntax":"swift"},{"type":"heading","text":"More information about","level":3,"anchor":"More-information-about"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"isActive":true,"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/Step5WorkWithPeripheral","overridingTitle":"Peripheral","overridingTitleInlineContent":[{"text":"Peripheral","type":"text"}],"type":"reference"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"overridingTitleInlineContent":[{"type":"text","text":"GPSService"}],"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/Step6UsingNTRIP","type":"reference","isActive":true,"overridingTitle":"GPSService"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"overridingTitleInlineContent":[{"type":"text","text":"LaserService"}],"type":"reference","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/Step7LaserService","isActive":true,"overridingTitle":"LaserService"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"overridingTitle":"EnviromentDataService","type":"reference","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/EnviromentDataService","overridingTitleInlineContent":[{"type":"text","text":"EnviromentDataService"}],"isActive":true}]}]},{"content":[{"inlineContent":[{"overridingTitle":"Use GPSCoordinate + Transformation","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/GPSCoordinate","type":"reference","isActive":true,"overridingTitleInlineContent":[{"text":"Use GPSCoordinate + Transformation","type":"text"}]}],"type":"paragraph"}]}]},{"type":"heading","text":"Navigation","level":2,"anchor":"Navigation"},{"inlineContent":[{"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/GettingStarted","overridingTitle":"Up to Getting started with VigramSDK","overridingTitleInlineContent":[{"text":"Up to Getting started with VigramSDK","type":"text"}],"isActive":true,"type":"reference"}],"type":"paragraph"},{"inlineContent":[{"type":"reference","isActive":true,"overridingTitle":"✅ STEP 1 (Import VigramSDK)","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/Step1GettingStarted","overridingTitleInlineContent":[{"text":"✅ STEP 1 (Import VigramSDK)","type":"text"}]}],"type":"paragraph"},{"inlineContent":[{"overridingTitle":"✅ STEP 2 (SDK Authentication)","overridingTitleInlineContent":[{"text":"✅ STEP 2 (SDK Authentication)","type":"text"}],"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/Step2SDKAuthentication","isActive":true,"type":"reference"}],"type":"paragraph"},