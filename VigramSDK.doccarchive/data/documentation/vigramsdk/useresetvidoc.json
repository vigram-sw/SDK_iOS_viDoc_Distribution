{"kind":"article","primaryContentSections":[{"content":[{"text":"","type":"heading","level":2,"anchor":""},{"type":"paragraph","inlineContent":[{"text":"The viDoc reset procedure returns the settings to factory defaults.","type":"text"}]},{"type":"paragraph","inlineContent":[{"inlineContent":[{"text":"Note:","type":"text"}],"type":"strong"},{"text":" Perform this procedure only if absolutely necessary.","type":"text"}]},{"content":[{"inlineContent":[{"type":"text","text":"The reset procedure takes approximately 1 minute."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"At the beginning of the process, all messages are interrupted."}],"type":"paragraph"},{"inlineContent":[{"text":"Disconnection from all satellites occurs.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"After the viDoc is reset, sending messages resumes."}],"type":"paragraph"},{"inlineContent":[{"text":"You must wait to receive the full NMEA GGA message.","type":"text"}],"type":"paragraph"}],"type":"aside","style":"note","name":"Note"},{"type":"paragraph","inlineContent":[{"type":"text","text":"To track the reset process, you need to use publisher"}]},{"syntax":"swift","code":["private var cancellables = Set<AnyCancellable>()","","peripheral.viDocResetState","    .sink(receiveCompletion: { completion in","        switch completion {","        case .finished:","            break \/\/ normal completion","        case .failure(let error):","            \/\/ do something with error","        }","    }) { [weak self] viDocResetState in","            switch viDocResetState {","            case let .isReseting(value):","                if value {","                    \/\/ reset in progress","                } else {","                    \/\/ reset is finished","                }","            case .failure: ","                \/\/ Error of reset viDoc","                break","            }","    }.store(in: &cancellables)"],"type":"codeListing"},{"type":"paragraph","inlineContent":[{"text":"To start the reset process, you need to perform the following method:","type":"text"}]},{"syntax":"swift","code":["peripheral.resetViDoc()"],"type":"codeListing"}],"kind":"content"}],"hierarchy":{"paths":[["doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK"]]},"identifier":{"url":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/UseResetVidoc","interfaceLanguage":"swift"},"sections":[],"metadata":{"modules":[{"name":"VigramSDK"}],"title":"Use Reset viDoc","images":[{"identifier":"logo.png","type":"icon"}],"roleHeading":"Article","role":"article"},"schemaVersion":{"patch":0,"major":0,"minor":3},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/vigramsdk\/useresetvidoc"]}],"references":{"logo.png":{"type":"image","identifier":"logo.png","alt":"Vigram logo.","variants":[{"url":"\/images\/com.vigram.VigramSDK\/logo.png","traits":["1x","light"]}]},"doc://com.vigram.VigramSDK/documentation/VigramSDK":{"kind":"symbol","role":"collection","type":"topic","url":"\/documentation\/vigramsdk","title":"VigramSDK","abstract":[],"images":[{"type":"icon","identifier":"logo.png"}],"identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK"}}}