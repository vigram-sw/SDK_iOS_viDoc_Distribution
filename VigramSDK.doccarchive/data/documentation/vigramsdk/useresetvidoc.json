{"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"primaryContentSections":[{"content":[{"level":2,"type":"heading","anchor":"","text":""},{"type":"paragraph","inlineContent":[{"type":"text","text":"The viDoc reset procedure returns the settings to factory defaults."}]},{"type":"paragraph","inlineContent":[{"inlineContent":[{"text":"Note:","type":"text"}],"type":"strong"},{"type":"text","text":" Perform this procedure only if absolutely necessary."}]},{"style":"note","content":[{"inlineContent":[{"text":"The reset procedure takes approximately 1 minute.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"At the beginning of the process, all messages are interrupted.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Disconnection from all satellites occurs."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"After the viDoc is reset, sending messages resumes.","type":"text"}]},{"inlineContent":[{"text":"You must wait to receive the full NMEA GGA message.","type":"text"}],"type":"paragraph"}],"name":"Note","type":"aside"},{"type":"paragraph","inlineContent":[{"text":"To track the reset process, you need to use publisher","type":"text"}]},{"syntax":"swift","code":["private var cancellables = Set<AnyCancellable>()","","peripheral.viDocResetState","    .sink(receiveCompletion: { completion in","        switch completion {","        case .finished:","            break \/\/ normal completion","        case .failure(let error):","            \/\/ do something with error","        }","    }) { [weak self] viDocResetState in","            switch viDocResetState {","            case let .isReseting(value):","                if value {","                    \/\/ reset in progress","                } else {","                    \/\/ reset is finished","                }","            case .failure: ","                \/\/ Error of reset viDoc","                break","            }","    }.store(in: &cancellables)"],"type":"codeListing"},{"inlineContent":[{"text":"To start the reset process, you need to perform the following method:","type":"text"}],"type":"paragraph"},{"type":"codeListing","syntax":"swift","code":["peripheral.resetViDoc()"]}],"kind":"content"}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK\/UseResetVidoc"},"hierarchy":{"paths":[["doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK"]]},"kind":"article","metadata":{"role":"article","title":"Use Reset viDoc","images":[{"type":"icon","identifier":"logo.png"}],"roleHeading":"Article","modules":[{"name":"VigramSDK"}]},"variants":[{"paths":["\/documentation\/vigramsdk\/useresetvidoc"],"traits":[{"interfaceLanguage":"swift"}]}],"references":{"logo.png":{"type":"image","alt":"Vigram logo.","variants":[{"url":"\/images\/com.vigram.VigramSDK\/logo.png","traits":["1x","light"]}],"identifier":"logo.png"},"doc://com.vigram.VigramSDK/documentation/VigramSDK":{"role":"collection","title":"VigramSDK","kind":"symbol","images":[{"identifier":"logo.png","type":"icon"}],"abstract":[],"url":"\/documentation\/vigramsdk","type":"topic","identifier":"doc:\/\/com.vigram.VigramSDK\/documentation\/VigramSDK"}}}